apiVersion: networking.k8s.io/v1
kind: Ingress
metadata:
  namespace: {{ namespace }}-{{ env }}
  name: {{ project_name }}-ingress
spec:
  rules:
  - host: {{ project_name }}.k8s.{{ env }}.gazeta.pl
    http:
      paths:
      - backend:
          service:
            name: {{ project_name }}
            port:
              number: {{ container_port }}
        path: /
        pathType: Prefix
